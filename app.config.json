{
  "name": "shared-notes",
  "displayName": "Shared Notes",
  "expo": {
    "name": "Shared Notes",
    "slug": "shared-notes",
    "version": "6.1",
    "platforms": ["ios"],
    "assetBundlePatterns": ["assets/*"],
    "userInterfaceStyle": "automatic",
    "scheme": "sharednotes",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 3000
    },
    "ios": {
      "bundleIdentifier": "com.mrarich.SharedNoteWidget"
    },
    "extra": {
      "MyVersion": "18"
    }
  }
}
