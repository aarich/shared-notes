{
  "name": "shared-notes",
  "displayName": "Shared Notes",
  "expo": {
    "name": "Shared Notes",
    "slug": "shared-notes",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "userInterfaceStyle": "automatic",
    "scheme": "sharednotes",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 3000
    },
    "ios": {
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-6949812709353975~7748559513"
      },
      "bundleIdentifier": "com.mrarich.SharedNoteWidget"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-6949812709353975~9033721022"
      },
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "package": "com.mrarich.SharedNoteWidget"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "MyVersion": "1"
    }
  }
}
